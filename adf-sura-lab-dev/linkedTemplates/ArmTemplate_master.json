{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-sura-lab-dev"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"ds_sourceazuresqldev_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ds_sourceazuresqldev'"
		},
		"ls_sourceADLSCopyFiles_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_sourceADLSCopyFiles'"
		},
		"AzureKeyVault_sql_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvaultlabadf.vault.azure.net/"
		},
		"ls_keyvault_to_sql_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "labsqlsecretname"
		},
		"ls_sourceADLSCopyFiles_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://suralabadlsdev.dfs.core.windows.net"
		},
		"TumblingWindowTrigger_properties_pipeline_parameters_startdate": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"TumblingWindowTrigger_properties_pipeline_parameters_enddate": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-sura-lab-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"ds_sourceazuresqldev_connectionString": {
						"value": "[parameters('ds_sourceazuresqldev_connectionString')]"
					},
					"ls_sourceADLSCopyFiles_accountKey": {
						"value": "[parameters('ls_sourceADLSCopyFiles_accountKey')]"
					},
					"AzureKeyVault_sql_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_sql_properties_typeProperties_baseUrl')]"
					},
					"ls_sourceADLSCopyFiles_properties_typeProperties_url": {
						"value": "[parameters('ls_sourceADLSCopyFiles_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-sura-lab-dev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_keyvault_to_sql_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_keyvault_to_sql_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-sura-lab-dev_ArmTemplate_0')]"
			]
		},
		{
			"name": "adf-sura-lab-dev_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-sura-lab-dev_ArmTemplate_1')]"
			]
		},
		{
			"name": "adf-sura-lab-dev_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"TumblingWindowTrigger_properties_pipeline_parameters_startdate": {
						"value": "[parameters('TumblingWindowTrigger_properties_pipeline_parameters_startdate')]"
					},
					"TumblingWindowTrigger_properties_pipeline_parameters_enddate": {
						"value": "[parameters('TumblingWindowTrigger_properties_pipeline_parameters_enddate')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-sura-lab-dev_ArmTemplate_2')]"
			]
		}
	]
}