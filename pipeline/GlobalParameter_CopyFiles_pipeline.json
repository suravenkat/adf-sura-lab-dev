					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_SourceCopyFiles",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_globalparameter_targetadls",
						"type": "DatasetReference",
						"parameters": {
							"container": {
								"value": "@pipeline().globalParameters.container",
								"type": "Expression"
							},
							"targetpath": {
								"value": "@pipeline().globalParameters.targetpath",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2024-02-08T03:30:54Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}